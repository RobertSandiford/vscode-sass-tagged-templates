{
	"name": "vscode-sass-tagged-templates",
	"displayName": "Sass Tagged Template Literals",
	"publisher": "Robert Sandiford",
	"license": "MIT",
	"version": "0.0.8",
	"description": "Syntax highlighting for CSS and SASS tagged template literals.",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"javascript",
		"jsx",
		"typescript",
		"tsx",
		"markdown",
		"mdx",
		"css",
		"sass",
		"scss",
		"template string",
		"syntax highlighting"
	],
	"engines": {
		"vscode": "^1.5.0"
	},
	"contributes": {
		"grammars": [
			{
				"injectTo": [
					"source.js",
					"source.ts",
					"source.jsx",
					"source.js.jsx",
					"source.tsx",
					"source.vue"
				],
				"path": "./syntaxes/sass-template.json",
				"scopeName": "sass-template"
			},
            {
                "injectTo": [
                    "source.js",
                    "source.ts",
                    "source.jsx",
                    "source.js.jsx",
                    "source.tsx",
                    "source.vue"
                ],
                "path": "./syntaxes/scss-template.json",
                "scopeName": "scss-template"
            },
            {
                "injectTo": [
                    "source.js",
                    "source.ts",
                    "source.jsx",
                    "source.js.jsx",
                    "source.tsx",
                    "source.vue"
                ],
                "path": "./syntaxes/css-template.json",
                "scopeName": "css-template"
            }
		]
	},
	"scripts": {
		"prepare": "node ./node_modules/vscode/bin/install"
	}
}
