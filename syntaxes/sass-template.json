{
	"injectionSelector": "L:source -comment -string",
	"patterns": [
		{
			"contentName": "source.css.scss",
			"begin": "(?:css|sass|scss)(?:\\.[$A-Za-z_][$A-Za-z0-9_]+)*(`)",
			"end": "`",
			"patterns": [
				{
					"include": "source.css.scss"
				}
			]
		}
	],
	"scopeName": "sass-template"
}
